<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">List&lt;T&gt; arrayList = new ArrayList&lt;T&gt;();</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">T[] t = (T[]) arrayList.toArray(typeof(T));</span></span></p>

<h4 style="text-align:justify"><span style="font-size:16pt"><span style="font-family:&quot;等线 Light&quot;"><span style="color:#538135">2. 数组转集合</span></span></span></h4>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">T[] t = new T[size];</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">List&lt;T&gt; l = Arrays.asList(t);</span></span></p>

<p style="text-align:justify">&nbsp;</p>

<h4 style="text-align:justify"><span style="font-size:16pt"><span style="font-family:&quot;等线 Light&quot;"><span style="color:#538135">实例</span></span></span></h4>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">public class Myclass {</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp; public static void main(Stringll args)</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp; {</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; List&lt;string&gt; a = new ArrayList&lt;string&gt;()</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; a.add(&quot;ａ&quot;);</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; a.add(&quot;ｂ&quot;);</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; a.add(&quot;ｃ&quot;);</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; int size =a.size;</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; string[] array =(String[])a.toArray(new String[size]);</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; System.out.println(array[0]);</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; List&lt;string&gt; list = Arrays.asList(array);</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; System.out.println(list.get(0));</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">&nbsp;&nbsp;&nbsp; }</span></span></p>

<p style="text-align:justify"><span style="font-size:10.5pt"><span style="font-family:等线">}</span></span></p>
